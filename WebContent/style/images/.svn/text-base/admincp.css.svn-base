/* common */
*{ word-wrap:break-word; outline:none; }
body{ background:#FFF; text-align:center; }
body, td, input, textarea, select,button{ color:#666; font:12px "Lucida Grande", Verdana, Lucida, Helvetica, Arial, 'Simsun', sans-serif; }
body, ul, dl, dd, p, h1, h2, h3, h4, h5, h6, form, fieldset { margin:0; padding:0; }
h1, h2, h3, h4, h5, h6{ font-size:12px; }
a{ color:#15C; text-decoration:none; }
	a:hover { text-decoration:underline; }
	a img{ border:none; }
em, cite, th{ font-style:normal; font-weight:normal; }
table{ border-collapse:collapse; }
.tb {clear: both;margin-top: 4px;width: 100%;}
.header, .header td, .header th {border-top: 1px dotted #DEEFFB;font-weight: 700;}
.tb2 th {font-size: 12px;line-height: 21px;padding: 5px 5px 3px 0;}
.tb2 td {border-top: 1px dotted #E5E5E5;padding: 5px 5px 3px 0;}
.td25 {width: 50px;}
.td24 {width: 120px;}
.tb2 .partition {background: url("bg_repx_hc.gif") repeat-x scroll 0 -40px transparent; padding: 5px;}
.partition, .partition2 {color: #0099CC;font-weight: 700;line-height: 21px;}

/* form */
textarea, input{ padding:2px; border:1px solid; border-color:#666 #eee #eee #666; }
input.txt:hover, input.txt:focus, textarea:hover, textarea:focus{ border-color:#A0A0A0; background:#FFFFFF; }
.radio{ margin-top:-1px; border:none; vertical-align:middle; }
.checkbox{ height:14px; border:none; vertical-align:middle; }
.btns{ margin:3px 0; padding:2px 5px; *padding:4px 5px 1px; border-color:#ddd #666 #666 #ddd; background:#DDD; color:#000; cursor:pointer; }
.btns_disable{margin:3px 0;}
.btns, .btns_disable, .sgbtn{
    border-color:#DADADA;
    -moz-user-select: none;
    background-color: #F5F5F5;
    background-image: -moz-linear-gradient(center top , #F5F5F5, #F1F1F1);
    border: 1px solid rgba(0, 0, 0, 0.1);
    border-radius: 2px 2px 2px 2px;
    color: #666666;
    cursor: default;
    font-family: arial,sans-serif;
    font-size: 12px;
    font-weight: bold;
    height: 29px;
    line-height: 27px;
    min-width: 54px;
    padding: 0 8px;
    text-align: center;
}
.btns:hover,.sgbtn:hover{
    background-color: #F8F8F8;
    background-image: -moz-linear-gradient(center top , #F8F8F8, #F1F1F1);
    border: 1px solid #C6C6C6;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    color: #333333;
}
.sgbtn{ margin-left:10px; color:#777777;padding:8px;border: 1px solid #C6C6C6;}
	.sgbtn:hover{ text-decoration:none; }
@media screen and (-webkit-min-device-pixel-ratio:0) { 
    .btns,.btns_disable{
        -moz-user-select: none;
        background-color: #F5F5F5;
        background-image: -moz-linear-gradient(center top , #F5F5F5, #F1F1F1);
        border: 1px solid rgba(0, 0, 0, 0.1);
        border-radius: 2px 2px 2px 2px;
        color: #666666;
        cursor: default;
        font-family: arial,sans-serif;
        font-size: 11px;
        font-weight: bold;
        height: 29px;
        line-height: 27px;
        min-width: 54px;
        padding: 0 8px;
        text-align: center;
    }
}

